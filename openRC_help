
# Проверка отказавших служб.
rc-status --crashed

# Проверка служб.
rc-status

# Отображение всех доступных служб.
rc-update -v show

# Изменить состояние службы.
rc-service "myservice" (start, stop, restart, status)

# Включить или выключить службу.
rc-update (add, del) "myservice"

# Включить или выключить службу(юзал это)
rc-update add "myservice" default

 df -h

